alias buildall='mvn clean install -Plocal -T 2C -Dassembly.skipAssembly=true -DskipTests'
alias f='fab -f scripts/fabfile.py'

alias logsh='docker run -ti --rm --volumes-from=data-volumes busybox'

alias start_ls='fab -f start_local_stack.py start_local_stack'
alias stop_ls='fab -f start_local_stack.py stop_local_stack'
