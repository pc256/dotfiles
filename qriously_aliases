alias qriously-server='cd $QRIOUSLY_HOME/QriouslyServer'
alias qriously-qdash='cd $QRIOUSLY_HOME/qdash'
alias qriously-random='cd $QRIOUSLY_HOME/randomProjects'

alias buildall='mvn clean install -Plocal -T 2C -Dassembly.skipAssembly=true -pl \!:qdash-webapp -DskipTests'
alias f='fab -f scripts/fabfile.py'

alias logsh='docker run -ti --rm --volumes-from=data-volumes busybox'

alias startls='fab -f start_local_stack.py start_local_stack'
alias stopls='fab -f start_local_stack.py stop_local_stack'
